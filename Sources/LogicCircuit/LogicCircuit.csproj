<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <AnalysisMode>AllEnabledByDefault</AnalysisMode>
    <UseWPF>true</UseWPF>
    <NeutralLanguage>en</NeutralLanguage>
    <AnalysisLevel>latest-all</AnalysisLevel>
    <ApplicationIcon>Properties\LogicCircuit.ico</ApplicationIcon>

    <Version>2.23.04.12</Version>
    <Description>www.LogicCircuit.org</Description>
    <Copyright>© LogicCircuit.org</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Properties\default.wav" />
    <None Remove="Properties\LogicCircuit.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Properties\LogicCircuit.ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Properties\default.wav" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="IronPython" Version="2.7.12" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Properties\LogicCircuit.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
